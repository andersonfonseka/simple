<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<simple-config>

	<simple-form name="projectForm" className="com.andersonfonseka.project.form.ProjectForm"/>
	<simple-form name="healthImagingForm" className="com.andersonfonseka.project.form.HealthImagingForm"/>
	
	<simple-forward name="project" path="/pages/project/project.jsp"/>
	<simple-forward name="projectPage" path="/pages/project/projectForm.jsp"/>
	
	<simple-forward name="healthImaging" path="/pages/healthImaging/healthImaging.jsp"/>
	

	<simple-controller name="projectController" 
					   className="com.andersonfonseka.project.controller.ProjectController"
					   formName="projectForm"
					   scope="request"/>
	
	<simple-controller name="healthImagingController" 
					   className="com.andersonfonseka.project.controller.HealthImagingController"
					   formName="healthImagingForm"
					   scope="request"/>


</simple-config>
